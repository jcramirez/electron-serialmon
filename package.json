{
  "name": "electron-serialport",
  "version": "1.0.2",
  "description": "A minimal Electron application serial monitor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "package-all": "electron-packager .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --prune=true",
    "package-win": "electron-packager . --overwrite --platform=win32 --prune=true --asar=true",
    "package-linux": "electron-packager . --overwrite --platform=linux --prune=true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcramirez/electron-serialport.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "jcramirez",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^9.0.1",
    "electron-rebuild": "^1.4.0",
    "standard": "*"
  },
  "dependencies": {
    "serialport": "^4.0.7"
  }
}
